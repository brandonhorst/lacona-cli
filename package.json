{
  "name": "lacona-cli",
  "version": "1.0.0",
  "description": "CLI Tool for building Lacona addons",
  "main": "lib/index.js",
  "bin": {
    "lacona": "lib/index.js"
  },
  "os": [
    "darwin"
  ],
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf lib",
    "lint": "standard src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonhorst/lacona-cli.git"
  },
  "keywords": [
    "lacona",
    "test",
    "cli",
    "build"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-cli/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-cli#readme",
  "dependencies": {
    "commander": "^2.9.0",
    "inquirer": "^1.1.3",
    "jsonfile": "^2.3.1",
    "lodash": "^4.15.0",
    "npm-safe-name": "^1.1.0",
    "update-notifier": "^1.0.2",
    "user-home": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "rimraf": "^2.5.4",
    "standard": "^8.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
